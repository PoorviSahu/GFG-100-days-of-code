// https://practice.geeksforgeeks.org/problems/k-th-element-of-two-sorted-array1317/1

#include <bits/stdc++.h>
using namespace std;

class Solution{
    public:
    int kthElement(int arr1[], int arr2[], int n, int m, int k)
    {
        int l,c[n+m];
        for(int i=0; i<n; i++)
       {
          c[i] = arr1[i];   
       }
       for(int i=n; i<n+m; i++)
       {
          c[i] = arr2[i-n];   
       }
       sort(c, c + (n+m));
       l=c[k-1];
    }
};


int main()
{
	int t;
	cin>>t;
	while(t--){
		int n,m,k;
		cin>>n>>m>>k;
		int arr1[n],arr2[m];
		for(int i=0;i<n;i++)
			cin>>arr1[i];
		for(int i=0;i<m;i++)
			cin>>arr2[i];
		
		Solution ob;
        cout << ob.kthElement(arr1, arr2, n, m, k)<<endl;
	}
    return 0;
}  
